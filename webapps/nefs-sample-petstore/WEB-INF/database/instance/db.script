CREATE INDEX PK_RECORD_STATUS ON RECORD_STATUS(ID)
CREATE CACHED TABLE PROFILE(PROFILE_ID VARCHAR(32) NOT NULL PRIMARY KEY,ACCOUNT_ID INTEGER,LANGPREF INTEGER NOT NULL,FAVCATEGORY VARCHAR(32),MYLSTOPT VARCHAR(32),BANNEROPT VARCHAR(32))
CREATE INDEX PK_PROFILE ON PROFILE(PROFILE_ID)
CREATE INDEX PR_PROFILES_ACCOUNT_ID ON PROFILE(ACCOUNT_ID)
CREATE CACHED TABLE ACCOUNT(ACCOUNT_ID INTEGER NOT NULL IDENTITY PRIMARY KEY,LOGIN_ID VARCHAR(32) NOT NULL,EMAIL VARCHAR(30) NOT NULL,FIRSTNAME VARCHAR(20) NOT NULL,LASTNAME VARCHAR(25) NOT NULL,STATUS VARCHAR(32) NOT NULL,ADDR1 VARCHAR(64) NOT NULL,ADDR2 VARCHAR(64) NOT NULL,CITY VARCHAR(64) NOT NULL,STATE VARCHAR(64) NOT NULL,COUNTRY VARCHAR(64) NOT NULL,PHONE VARCHAR(30) NOT NULL)
CREATE INDEX PK_ACCOUNT ON ACCOUNT(ACCOUNT_ID)
CREATE CACHED TABLE BANNERDATA(BANNERDATA_ID VARCHAR(32) NOT NULL PRIMARY KEY,BANNERNAME VARCHAR(64) NOT NULL)
CREATE INDEX PK_BANNERDATA ON BANNERDATA(BANNERDATA_ID)
CREATE CACHED TABLE CATEGORY(CATEGORY_ID VARCHAR(32) NOT NULL PRIMARY KEY,NAME VARCHAR(30) NOT NULL,DESCR VARCHAR(30) NOT NULL)
CREATE INDEX PK_CATEGORY ON CATEGORY(CATEGORY_ID)
CREATE CACHED TABLE PRODUCT(PRODUCT_ID VARCHAR(32) NOT NULL PRIMARY KEY,CATEGORY_ID VARCHAR(32),NAME VARCHAR(30) NOT NULL,DESCR VARCHAR(30) NOT NULL,CONSTRAINT FK_PRODUCTS_CATEGORY_ID FOREIGN KEY(CATEGORY_ID) REFERENCES CATEGORY(CATEGORY_ID) ON DELETE CASCADE)
