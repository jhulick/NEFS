CREATE INDEX PK_ITEM ON ITEM(ITEM_ID)
CREATE INDEX PR_ITEMS_PRODUCT_ID ON ITEM(PRODUCT_ID)
CREATE CACHED TABLE SUPPLIER(SUPPLIER_ID INTEGER NOT NULL IDENTITY PRIMARY KEY,NAME VARCHAR(64) NOT NULL,STATUS VARCHAR(20) NOT NULL,ADDR1 VARCHAR(64) NOT NULL,ADDR2 VARCHAR(64) NOT NULL,CITY VARCHAR(64) NOT NULL,STATE VARCHAR(64) NOT NULL,COUNTRY VARCHAR(64) NOT NULL,PHONE VARCHAR(30) NOT NULL)
CREATE INDEX PK_SUPPLIER ON SUPPLIER(SUPPLIER_ID)
CREATE CACHED TABLE ORDERS(ORDERS_ID INTEGER NOT NULL IDENTITY PRIMARY KEY,ACCOUNT_ID INTEGER,ORDERDATE DATE NOT NULL,SHIPADDR1 VARCHAR(64) NOT NULL,SHIPADDR2 VARCHAR(64) NOT NULL,SHIPCITY VARCHAR(64) NOT NULL,SHIPSTATE VARCHAR(64) NOT NULL,SHIPCOUNTRY VARCHAR(64) NOT NULL,SHIPPHONE VARCHAR(30) NOT NULL,BILLADDR1 VARCHAR(64) NOT NULL,BILLADDR2 VARCHAR(64) NOT NULL,BILLCITY VARCHAR(64) NOT NULL,BILLSTATE VARCHAR(64) NOT NULL,BILLCOUNTRY VARCHAR(64) NOT NULL,BILLPHONE VARCHAR(30) NOT NULL,CONSTRAINT FK_ORDS_ACCOUNT_ID FOREIGN KEY(ACCOUNT_ID) REFERENCES ACCOUNT(ACCOUNT_ID) ON DELETE CASCADE)
CREATE INDEX PK_ORDERS ON ORDERS(ORDERS_ID)
CREATE INDEX PR_ORDS_ACCOUNT_ID ON ORDERS(ACCOUNT_ID)
CREATE CACHED TABLE ORDERSTATUS(ORDERSTATUS_ID INTEGER NOT NULL IDENTITY PRIMARY KEY,ORDERS_ID INTEGER,ITEMNUM INTEGER NOT NULL,TS DATE NOT NULL,STATUS VARCHAR(20) NOT NULL,CONSTRAINT FK_ORDERSTAT_ORDERS_ID FOREIGN KEY(ORDERS_ID) REFERENCES ORDERS(ORDERS_ID) ON DELETE CASCADE)
CREATE INDEX PK_ORDERSTATUS ON ORDERSTATUS(ORDERSTATUS_ID)
CREATE INDEX PR_ORDERSTAT_ORDERS_ID ON ORDERSTATUS(ORDERS_ID)
CREATE CACHED TABLE LINEITEM(LINEITEM_ID INTEGER NOT NULL IDENTITY PRIMARY KEY,ORDERS_ID INTEGER,ITEMNUM INTEGER,ITEM_ID VARCHAR(32),QUANTITY INTEGER NOT NULL,UNITPRICE FLOAT NOT NULL,CONSTRAINT FK_LINEITEMS_ITEM_ID FOREIGN KEY(ITEM_ID) REFERENCES ITEM(ITEM_ID) ON DELETE CASCADE,CONSTRAINT FK_LINEITEMS_ORDERS_ID FOREIGN KEY(ORDERS_ID) REFERENCES ORDERS(ORDERS_ID) ON DELETE CASCADE)
CREATE INDEX PK_LINEITEM ON LINEITEM(LINEITEM_ID)
CREATE INDEX PR_LINEITEMS_ORDERS_ID ON LINEITEM(ORDERS_ID)
CREATE INDEX PR_LINEITEMS_ITEM_ID ON LINEITEM(ITEM_ID)
