<?xml version="1.0" encoding="UTF-8" ?>
<!ENTITY % boolean "(true | false | on | off | yes | no)">
<!ENTITY % elements "dal">

<!ELEMENT dal (person | product)*>

<!ELEMENT person (cr-stamp | cr-person-id | record-status-id | system-id | person-id | name-first | name-last | password | dal:include)*>
<!ATTLIST person
          ID CDATA #IMPLIED
          IDREF CDATA #IMPLIED
          cr-stamp CDATA #IMPLIED
          cr-person-id CDATA #IMPLIED
          record-status-id (0 | Inactive | 1 | Active | 99 | Unknown) #IMPLIED
          system-id CDATA #IMPLIED
          person-id CDATA #IMPLIED
          name-first CDATA #IMPLIED
          name-last CDATA #IMPLIED
          password CDATA #IMPLIED>

