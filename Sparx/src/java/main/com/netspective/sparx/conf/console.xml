            <panels>
                <panel class="com.netspective.sparx.console.panel.XdmComponentsPanel"/>
            </panels>
        </page>
        <page name="application" caption="Application">
	        <page name="metrics" caption="Metrics"/>
	        <page name="configuration" caption="Configuration Properties"/>
