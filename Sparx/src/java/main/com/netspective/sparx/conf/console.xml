                <panels>
                    <panel class="com.netspective.sparx.console.panel.XdmComponentsPanel"/>
                </panels>
            </page>
	        <page name="value-source" caption="Value Sources">
                <panels>
                    <panel class="com.netspective.sparx.console.panel.ValueSourcesDocumentationPanel"/>
                    <panel class="com.netspective.sparx.console.panel.ValueSourcesUsagePanel"/>
                </panels>
