digraph G {
	edge [fontname="Helvetica",fontsize=10,labelfontname="Helvetica",labelfontsize=10];
	node [fontname="Helvetica",fontsize=10,shape=record];
	// School
	c3 [label="{School\n|name : Name\laddress : String\lphone : Number\l|addStudent()\lremoveStudent()\lgetStudent()\lgetAllStudents()\laddDepartment()\lremoveDepartment()\lgetDepartment()\lgetAllDepartments()\l}"];
	// School has Student
	c3 -> c4 [taillabel="1..*", label="Member", headlabel="*", arrowhead=none, arrowtail=ediamond]
	// School composed Department
	c3 -> c5 [taillabel="1..*", label="Has", headlabel="1..*", arrowhead=none, arrowtail=diamond]
	// Department
	c5 [label="{Department\n|name : Name\l|addInstructor()\lremoveInstructor()\lgetInstructor()\lgetAllInstructors()\l}"];
	// Department assoc Course
	c5 -> c6 [taillabel="1..*", label="", headlabel="1..*", arrowhead=none]
	// Department assoc Instructor
	c5 -> c7 [taillabel="0..*", label="", headlabel="0..1 chairperson", arrowhead=none]
	// Department has Instructor
	c5 -> c7 [taillabel="1..*", label="AssignedTo", headlabel="1..*", arrowhead=none, arrowtail=ediamond]
	// Student
	c4 [label="{Student\n|name : Name\lstudentID : Number\l|}"];
	// Student assoc Course
	c4 -> c6 [taillabel="*", label="Attends", headlabel="*", arrowhead=none]
	// Course
	c6 [label="{Course\n|name : Name\lcourseID : Number\l|}"];
	// Instructor
	c7 [label="{Instructor\n|name : Name\l|}"];
	// Instructor assoc Course
	c7 -> c6 [taillabel="1..*", label="Teaches", headlabel="*", arrowhead=none]
}

