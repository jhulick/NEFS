                <version>3.0.9</version>
                <class-path>mysql.jar</class-path>
                <source home-uri="http://www.mysql.com/products/connector-j/index.html"
                    license="GPL"/>
            </library>

            <library>
                <name>ZQL SQL Parser Library</name>
                <version>1.0</version>
                <class-path>zql.jar</class-path>
                <source home-uri="mailto:pierreyves.gibello@experlog.com"
                    license="Open Source, Free, Custom License"/>
            </library>
        </dependencies>
    </product>
